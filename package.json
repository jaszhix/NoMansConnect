{
  "name": "no-mans-connect",
  "productName": "NoMansConnect",
  "version": "1.5.10",
  "description": "A location manager for No Man's Sky.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=16192 --trace-warnings -r @babel/register -r @babel/polyfill  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r @babel/register -r @babel/polyfill ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register -r @babel/polyfill ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron --js-flags=\"--max_old_space_size=16192\" ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron --max_old_space_size=16192 -r @babel/register -r @babel/polyfill ./app/main.dev.hot",
    "postinstall": "concurrently \"npm run build\" \"npm run build-dll\" \"npm run install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "install-app-deps": "electron-builder install-app-deps",
    "dev": "cross-env START_HOT=1 npm run hot-server",
    "nsis": "electron-builder --win --x64",
    "pack-win": "electron-builder --publish never --dir",
    "build-linux": "electron-builder --linux",
    "build-win": "electron-builder --win --x64",
    "package": "npm run build && electron-builder --publish never",
    "package-win": "npm run build && electron-builder --win --x64",
    "package-linux": "npm run build && electron-builder --linux",
    "package-all": "npm run build && electron-builder -wl",
    "cleanup": "mop -v"
  },
  "browserslist": "electron 1.7",
  "build": {
    "productName": "NoMansConnect",
    "appId": "org.jaszhix.nmsc",
    "nsis": {
      "perMachine": false,
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "license": "LICENSE.md",
      "deleteAppDataOnUninstall": true
    },
    "asar": true,
    "files": [
      "dist/*",
      "node_modules/axios/*",
      "node_modules/lodash/*",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json",
      "ajax.worker.js",
      "fs.worker.js",
      "format.worker.js",
      "json.worker.js",
      "map.worker.js",
      "map3d.worker.js",
      "json.js",
      "each.js",
      "copy.js",
      "lang.js",
      "log.js",
      "constants.js"
    ],
    "extraFiles": [
      {
        "from": "./app/nmssavetool",
        "to": "./nmssavetool"
      },
      {
        "from": "./app/textures",
        "to": "./app/textures"
      },
      {
        "from": "./app/dist",
        "to": "./resources/dist"
      }
    ],
    "extraResources": [
      {
        "from": "node_modules/axios",
        "to": "node_modules/axios"
      },
      {
        "from": "node_modules/lodash",
        "to": "node_modules/lodash"
      },
      {
        "from": "node_modules/is-buffer",
        "to": "node_modules/is-buffer"
      },
      {
        "from": "node_modules/graceful-fs",
        "to": "node_modules/graceful-fs"
      },
      {
        "from": "node_modules/jszip",
        "to": "node_modules/jszip"
      },
      {
        "from": "node_modules/set-immediate-shim",
        "to": "node_modules/set-immediate-shim"
      },
      {
        "from": "node_modules/pako",
        "to": "node_modules/pako"
      }
    ],
    "dmg": {
      "icon": "./resources/icons/256x256.icns",
      "iconSize": 256,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Game",
      "description": "A location manager for No Man's Sky.",
      "target": [
        "deb",
        "pacman",
        "rpm"
      ],
      "desktop": {
        "type": "Application",
        "encoding": "UTF-8",
        "comment": "A location manager for No Man's Sky.",
        "exec": "nomansconnect",
        "icon": "./resources/icons/48x48.png"
      },
      "executableName": "nomansconnect"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaszhix/NoMansConnect.git"
  },
  "author": {
    "name": "Jason Hicks",
    "email": "jaszhix@gmail.com",
    "url": "https://github.com/jaszhix"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaszhix/NoMansConnect/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/jaszhix/NoMansConnect#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@hot-loader/react-dom": "^16.9.0-4.12.11",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.1",
    "@types/electron": "^1.6.10",
    "@types/graceful-fs": "^4.1.3",
    "@types/lodash": "^4.14.136",
    "@types/mathjs": "^6.0.1",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-tooltip": "^3.9.3",
    "@types/recharts": "^1.1.20",
    "@types/watch": "^1.0.1",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "asar": "2.0.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "4.1.1",
    "core-js": "3.2.1",
    "cross-env": "5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "3.2.0",
    "csstype": "^2.6.6",
    "devtron": "^1.4.0",
    "electron": "6.0.2",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "^3.10.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.14.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "expose-loader": "0.7.5",
    "express": "4.17.1",
    "fbjs-scripts": "1.2.0",
    "file-loader": "4.2.0",
