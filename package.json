    "vquery": "^5.0.1",
    "watch": "^1.0.2",
    "win-ps": "0.0.1"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.16.x"
  }
}
