{
  "name": "no-mans-connect",
  "productName": "NoMansConnect",
  "version": "1.2.1",
  "description": "A location manager for No Man's Sky.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=16192 --trace-warnings -r babel-register -r babel-polyfill  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r babel-register -r babel-polyfill ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register -r babel-polyfill ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron --js-flags=\"--max_old_space_size=16192\" ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron --max_old_space_size=16192 -r babel-register -r babel-polyfill ./app/main.development",
    "_postinstall": "concurrently \"npm run build\" \"npm run build-dll\" \"npm run install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "install-app-deps": "electron-builder install-app-deps",
    "dev": "cross-env START_HOT=1 npm run hot-server",
    "nsis": "build --win --x64",
    "pack-win": "build --publish never --dir",
    "build-linux": "build --linux",
    "build-win": "build --win --x64",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -wl",
    "cleanup": "mop -v"
  },
  "browserslist": "electron 1.7",
  "build": {
    "productName": "NoMansConnect",
    "appId": "org.jaszhix.nmsc",
    "nsis": {
      "perMachine": false,
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "license": "LICENSE.md",
      "deleteAppDataOnUninstall": true
    },
    "asar": true,
    "files": [
      "dist/",
      "node_modules/axios/*",
      "node_modules/lodash/*",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json",
      "ajaxWorker.js",
      "formatWorker.js",
      "jsonWorker.js",
      "settingsWorker.js",
      "mapWorker.js",
      "map3dWorker.js",
      "json.js",
      "each.js",
      "copy.js",
      "lang.js"
    ],
    "extraFiles": [
      {
        "from": "./app/nmssavetool",
        "to": "./nmssavetool"
      },
      {
        "from": "./app/textures",
        "to": "./app/textures"
      }
    ],
    "extraResources": [
      {
        "from": "node_modules/axios",
        "to": "node_modules/axios"
      },
      {
        "from": "node_modules/lodash",
        "to": "node_modules/lodash"
      },
      {
        "from": "node_modules/is-buffer",
        "to": "node_modules/is-buffer"
      }
    ],
    "dmg": {
      "icon": "./resources/icons/256x256.icns",
      "iconSize": 256,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Game",
      "description": "A location manager for No Man's Sky.",
      "target": [
        "deb",
        "pacman",
        "rpm"
      ],
      "desktop": {
        "type": "Application",
        "encoding": "UTF-8",
        "comment": "A location manager for No Man's Sky.",
        "exec": "nomansconnect",
        "icon": "./resources/icons/48x48.png"
      },
      "executableName": "nomansconnect"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaszhix/NoMansConnect.git"
  },
  "author": {
    "name": "Jason Hicks",
    "email": "jaszhix@gmail.com",
    "url": "https://github.com/jaszhix"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaszhix/NoMansConnect/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/jaszhix/NoMansConnect#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "asar": "0.14.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "3.6.0",
    "cross-env": "5.2.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "0.28.11",
    "devtron": "^1.4.0",
    "electron": "3.0.0-beta.4",
    "electron-builder": "20.27.0",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "^3.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-compat": "2.5.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "expose-loader": "0.7.5",
    "express": "4.16.3",
    "fbjs-scripts": "0.8.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jsdom": "^11.3.0",
    "minimist": "^1.2.0",
    "node-loader": "^0.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "16.4.2",
    "redux-logger": "^3.0.6",
    "sinon": "^4.0.1",
    "spectron": "3.8.0",
    "style-loader": "0.20.3",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-server": "2.9.3",
    "webpack-merge": "4.1.2",
    "worker-loader": "1.1.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "curlrequest": "^1.0.1",
    "dotenv": "^4.0.0",
    "electron-debug": "^2.0.0",
    "electron-window-state": "^4.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "font-awesome": "^4.7.0",
    "graceful-fs": "^4.1.11",
    "lodash": "4.17.10",
    "mathjs": "5.0.4",
    "moment": "2.22.2",
    "node-machine-id": "1.1.10",
    "open-external": "^0.1.1",
    "prop-types": "15.6.2",
    "raven-js": "3.26.4",
    "react": "16.4.2",
    "react-autobind": "^1.0.6",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "react-markdown": "^2.5.0",
    "react-onclickoutside": "6.7.1",
    "react-tooltip": "3.6.1",
    "recharts": "1.1.0",
    "resolve-dir": "^1.0.1",
    "source-map-support": "0.5.0",
    "threact": "0.4.7",
    "three": "0.95.0",
    "tween.js": "^16.6.0",
    "vquery": "^5.0.1",
    "watch": "^1.0.2",
    "win-ps": "0.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
